                        <Label Content="Масса тела" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="2"
                               Grid.Column="0" />
                        <TextBox x:Name="MassTB" Grid.Column="1" HorizontalAlignment="Center" Height="23"
                                 Grid.Row="2" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Grid.Row="2" Grid.Column="2">
                          <Label.Content>
                            <MultiBinding Converter="{StaticResource RadiusMassToDensityConverter}" ConverterParameter="Плотность шара: {0:N2}">
                              <Binding ElementName="RadiusTB" Path="Text"></Binding>
                              <Binding ElementName="MassTB" Path="Text"></Binding>
                            </MultiBinding>
                          </Label.Content>
                        </Label>

                        <Label Content="Радиус тела" HorizontalAlignment="Left" VerticalAlignment="Top"
                               Grid.Row="3"
                               Grid.Column="0" />
                        <TextBox x:Name="RadiusTB" Grid.Column="1" HorizontalAlignment="Center"
                                 Height="23"
                                 Grid.Row="3" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Content="Длина стержня" HorizontalAlignment="Left" VerticalAlignment="Top"
                               Grid.Row="4"
                               Grid.Column="0" />

                        <Label Grid.Row="4" Grid.Column="2">
                          <Label.Content>
                                <MultiBinding Converter="{StaticResource LengthToPeriodConverter}" ConverterParameter="Частота колебаний: {0:N2}">
                                    <Binding ElementName="LengthTB" Path="Text"></Binding>
                                </MultiBinding>
                          </Label.Content>
                        </Label>

                        <TextBox x:Name="LengthTB" Grid.Column="1" HorizontalAlignment="Center"
                                 Height="23"
                                 Grid.Row="4" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Content="Начальная скорость" HorizontalAlignment="Left" VerticalAlignment="Top"
                               Grid.Row="5"
                               Grid.Column="0" />
                        
                        <TextBox x:Name="InitialSpeedTB" Grid.Column="1" HorizontalAlignment="Center" Height="23"
                                 Grid.Row="5" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Content="Начальный угол" HorizontalAlignment="Left" VerticalAlignment="Top"
                               Grid.Row="6"
                               Grid.Column="0" />

                        <TextBox x:Name="InitialAngleTB" Grid.Column="1" HorizontalAlignment="Center" Height="23"
                                 Grid.Row="6" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Content="Амплитуда ветра силы" HorizontalAlignment="Left" VerticalAlignment="Top"
                               Grid.Row="7"
                               Grid.Column="0" />

                        <TextBox x:Name="WindAmplitudeTB" Grid.Column="1" HorizontalAlignment="Center"
                                 Height="23"
                                 Grid.Row="7" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput" TextChanged="ExternalForceAmplitudeTB_TextChanged"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}" />

                        <Label Content="Частота действия ветра" HorizontalAlignment="Left"
                               VerticalAlignment="Top" Grid.Row="8"
                               Grid.Column="0" />
                        
                        <TextBox x:Name="WindPeriodTB" Grid.Column="1" HorizontalAlignment="Center"
                                 Height="23"
                                 Grid.Row="8" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput" TextChanged="ExternalForcePeriodTB_TextChanged"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Content="Постоянный ветер" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="9"
                               Grid.Column="0" />
                        
                        <TextBox x:Name="ConstantWindTB" Grid.Column="1" HorizontalAlignment="Center"
                               Height="23"
                               Grid.Row="9" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="50"
                               PreviewTextInput="DoubleTBPreviewTextInput" TextChanged="ExternalForcePeriodTB_TextChanged"
                               PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>
                    
                        <Label Content="Импульс" HorizontalAlignment="Left"
                               VerticalAlignment="Top" Grid.Row="10"
                               Grid.Column="0" />
                        <TextBox x:Name="ImpulseTB" Grid.Column="1" HorizontalAlignment="Center"
                                 Height="23"
                                 Grid.Row="10" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="50"
                                 PreviewTextInput="DoubleTBPreviewTextInput"
                                 PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                         <Label Content="Плотность среды" HorizontalAlignment="Left"
                                 VerticalAlignment="Top" Grid.Row="11"
                                 Grid.Column="0" />
                         <TextBox x:Name="EnviromentDensityTB" Grid.Column="1" HorizontalAlignment="Center"
                                   Height="23"
                                   Grid.Row="11" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="50"
                                   PreviewTextInput="DoubleTBPreviewTextInput"
                                   PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                         <Label Content="Вязкость среды" HorizontalAlignment="Left"
                                 VerticalAlignment="Top" Grid.Row="12"
                                 Grid.Column="0" />
                         <TextBox x:Name="EnviromentViscosityTB" Grid.Column="1" HorizontalAlignment="Center"
                                   Height="23"
                                   Grid.Row="12" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="50"
                                   PreviewTextInput="DoubleTBPreviewTextInput"
                                   PreviewKeyDown="TB_PreviewKeyDown" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                          <Label Content="Сила Архимеда" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="13"
                                 Grid.Column="0" />
                          <CheckBox x:Name="ArchimedesForceCB" Grid.Column="1" Grid.Row="13" Margin="5"
                                    HorizontalAlignment="Center" Height="23" VerticalAlignment="Top"
                                    IsChecked="False" Foreground="{StaticResource IdealForegroundColorBrush}"/>

                        <Label Content="Сила сопротивления газа" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="14"
                               Grid.Column="0" />
                        <CheckBox x:Name="GasDragForceCB" Grid.Column="1" Grid.Row="14" Margin="5"
                                HorizontalAlignment="Center" Height="23" VerticalAlignment="Top"
                                IsChecked="False" Foreground="{StaticResource IdealForegroundColorBrush}"
                                IsEnabled="{qc:Binding '!$P', P={Binding ElementName=LiquidDragForceCB, Path=IsChecked}}"/>

                      <Label Content="Сила сопротивления жидкости" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="15"
                             Grid.Column="0" />
                      <CheckBox x:Name="LiquidDragForceCB" Grid.Column="1" Grid.Row="15" Margin="5"
                                HorizontalAlignment="Center" Height="23" VerticalAlignment="Top"
                                IsChecked="False" Foreground="{StaticResource IdealForegroundColorBrush}"
                                IsEnabled="{qc:Binding '!$P', P={Binding ElementName=GasDragForceCB, Path=IsChecked}}"/>

                    </Grid>
                </TabItem>
                <TabItem Header="ГРАФИК" x:Name="PlotTab">
                    <TabItem.Resources>
                        <s:ColorToBrushConverter x:Key="ColorToBrushConverter" />
                        <s:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
                        <DataTemplate x:Key="CustomLegendTemplate" DataType="s:SciChartLegend">
                            <Grid Margin="2,5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Ellipse Grid.Column="0" Width="10" Height="10" Margin="5,0,20,0"
                                         HorizontalAlignment="Left" VerticalAlignment="Center"
                                         Fill="{Binding Stroke, Converter={StaticResource ColorToBrushConverter}}" />
                                <TextBlock Grid.Column="1" Width="150" HorizontalAlignment="Center"
                                           Text="{Binding SeriesName}" />
                                <Grid Grid.Column="2"
                                      Visibility="{Binding DataContext.ShowVisibilityCheckboxes, ElementName=PART_LegendItems, Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <CheckBox HorizontalAlignment="Left" VerticalAlignment="Center" Content="Показать"
                                              Foreground="{Binding Stroke, Converter={StaticResource ColorToBrushConverter}}"
                                              IsChecked="{Binding IsVisible, Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </DataTemplate>

                    </TabItem.Resources>

                    <Grid>
                        <s:SciChartSurface x:Name="sciChartSurface">
                            <s:SciChartSurface.RenderableSeries>
                                <s:FastLineRenderableSeries x:Name="xSeries" Stroke="DarkOrange" StrokeThickness="3" />
                                <s:FastLineRenderableSeries x:Name="speedSeries" Stroke="SteelBlue" StrokeThickness="3" />
                                <s:FastLineRenderableSeries x:Name="energySeries" Stroke="DarkOliveGreen"
                                                            StrokeThickness="3" />
                            </s:SciChartSurface.RenderableSeries>

                            <s:SciChartSurface.XAxis>
                                <s:NumericAxis AutoRange="Always" AxisTitle="Время" TextFormatting="0.0" />
                            </s:SciChartSurface.XAxis>

                            <s:SciChartSurface.YAxis>
                                <s:NumericAxis AutoRange="Always" DrawMajorBands="True" GrowBy="0.1, 0.1"
                                               TextFormatting="0.00" />
                            </s:SciChartSurface.YAxis>

                            <s:SciChartSurface.ChartModifier>
                                <s:ModifierGroup>
                                    <s:CursorModifier/>
                                    <s:SeriesValueModifier/>
                                    <s:LegendModifier x:Name="legendModifier" Margin="10" GetLegendDataFor="AllSeries" LegendItemTemplate="{StaticResource CustomLegendTemplate}" ShowLegend="True" ShowVisibilityCheckboxes="True"/>
                                </s:ModifierGroup>
                            </s:SciChartSurface.ChartModifier>
                        </s:SciChartSurface>
                    </Grid>
                </TabItem>
                <TabItem Header="ФАЗОВЫЙ ПОРТРЕТ" x:Name="PhaseTab">
                    <TabItem.Resources>
                        <s:ColorToBrushConverter x:Key="ColorToBrushConverter"/>
                    </TabItem.Resources>
                    <Grid>
                        <s:SciChartSurface x:Name="sciPhaseChartSurface">
                            <s:SciChartSurface.RenderableSeries>
                                <s:FastLineRenderableSeries x:Name="phaseSeries" Stroke="DarkOrange" StrokeThickness="3"></s:FastLineRenderableSeries>
                            </s:SciChartSurface.RenderableSeries>

                            <s:SciChartSurface.XAxis>
                                <s:NumericAxis AutoRange="Always" AxisTitle="Координата" TextFormatting="0.0" />
                            </s:SciChartSurface.XAxis>

                            <s:SciChartSurface.YAxis>
                                <s:NumericAxis AutoRange="Always" AxisTitle="Скорсоть" DrawMajorBands="True"
                                               GrowBy="0.1, 0.1" TextFormatting="0.00" />
                            </s:SciChartSurface.YAxis>

                        </s:SciChartSurface>
                    </Grid>
                </TabItem>
                <TabItem Header="ДАННЫЕ">
                    <Grid ShowGridLines="False">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition />
                            <RowDefinition Height="30" />
                        </Grid.RowDefinitions>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>

                        <Grid.Resources>
                            <local:StringFormatConverter x:Key="StringFormatConverter" />
                        </Grid.Resources>

                        <GridSplitter Grid.Row="0" Grid.Column="1" Grid.RowSpan="7" Width="5"
                                      VerticalAlignment="Stretch"
                                      HorizontalAlignment="Center" />

                        <ListView Name="DataLV" ItemsSource="{Binding Data}" Grid.Row="0" Grid.Column="0"
                                  Grid.RowSpan="7">
                            <ListView.View>
                                <GridView>
                                    <GridView.Columns>
                                        <GridViewColumn Header="k" DisplayMemberBinding="{Binding PointNumber}" />
                                        <GridViewColumn DisplayMemberBinding="{Binding TimePoint}">
                                            <GridViewColumnHeader>
                                                <TextBlock>
                                                    t<Run BaselineAlignment="Subscript" FontSize="10">k</Run>
                                                </TextBlock>
                                            </GridViewColumnHeader>
                                        </GridViewColumn>
                                        <GridViewColumn
                                            DisplayMemberBinding="{Binding X, StringFormat=n3}">
                                            <GridViewColumnHeader>
                                                <TextBlock>
                                                    x<Run BaselineAlignment="Subscript" FontSize="10">k</Run>
                                                </TextBlock>
                                            </GridViewColumnHeader>
                                        </GridViewColumn>
                                        <GridViewColumn
                                            DisplayMemberBinding="{Binding V, StringFormat=n3}">
                                            <GridViewColumnHeader>
                                                <TextBlock>
                                                    v<Run BaselineAlignment="Subscript" FontSize="10">k</Run>
                                                </TextBlock>
                                            </GridViewColumnHeader>
                                        </GridViewColumn>
                                        <GridViewColumn DisplayMemberBinding="{Binding E, StringFormat=n3}">
                                            <GridViewColumnHeader>
                                                <TextBlock>
                                                    E<Run BaselineAlignment="Subscript" FontSize="10">k</Run>
                                                </TextBlock>
                                            </GridViewColumnHeader>
                                        </GridViewColumn>
                                    </GridView.Columns>
                                </GridView>
                            </ListView.View>
                        </ListView>
                        
                        <Button Name="SaveButton" Grid.Row="7" Grid.Column="0" HorizontalAlignment="Left"
                                Click="SaveButton_Click">
                            Сохранить
                        </Button>
                    </Grid>
                </TabItem>
              <TabItem Header="АНИМАЦИЯ" x:Name="animTab">
                <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                  </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Canvas Name="animCanvas" VerticalAlignment="Center" HorizontalAlignment="Center" Width="1280" Height="720" Grid.Row ="0" Grid.Column="0"/>
                </Grid>
              </TabItem>
            </dragablz:TabablzControl>
        </dockablz:Layout>
            <Button x:Name="StartBtn" Content="Старт" Grid.Column="0" Grid.Row="0" Click="StartBtn_OnClick" />
            <Button x:Name="StopBtn" Content="Стоп" Grid.Column="1" Grid.Row="0" Click="StopBtn_OnClick" IsEnabled="False" />
            <Button x:Name="PauseButton" Content="Приостановить" Grid.Column="2" Grid.Row="0" Click="PauseButton_Click" IsEnabled="False"/>
            <Button x:Name="ResumeButton" Content="Продолжить" Grid.Column="3" Grid.Row="0" Click="ResumeButton_Click"  IsEnabled="False"/>
            <Button x:Name="ImpulseBtn" Content="Импульс" Grid.Column="4" Grid.Row="0" Click="ImpulseBtn_OnClick"
                IsEnabled="False"/>
    </Grid>
</metro:MetroWindow>